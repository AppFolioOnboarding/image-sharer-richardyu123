<div class="album text-muted">
  <div class="container">
    <div class="row">
      <% @images.each do |img| %>
        <div class="card">
          <%= image_tag(img.link, width: "350")%>
          <a href="<%= image_share_path(img) %>" class="btn btn-primary">Share</a>
        </div>
      <% end %>
    </div>
  </div>
</div>

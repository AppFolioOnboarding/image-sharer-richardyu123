<div class="album text-muted">
  <div class="container">
    <div class="row">
      <% @images.each do |img| %>
        <div class="card">
          <%= image_tag(img.link, width: "350")%>
          <div class="card-body">
            <h5 class="card-title">Tags:</h5>
            <p class="card-text"><%=img.tag_list %></p>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
